from cryptography.fernet import Fernet

key = Fernet.genreate_key()
print("key", key)
f = Fernet(key)
plaintext = b"A really secret message. Not for prying eyes."
print("plaintext", plaintext)
ciphertext = f.encrypt(plaintext)
print("ciphertext", ciphertext)

